[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group osp-ceph-automation
	owner = group user/Yogev Rabl (yorabl)
        owner = group user/Tomas Rusnak (trusnak)
[access "refs/for/refs/*"]
	read = group osp-ceph-automation
	read = group user/Yogev Rabl (yorabl)
        read = group user/Tomas Rusnak (trusnak)
	push = group osp-ceph-automation
	push = group user/Yogev Rabl (yorabl)
        push = group user/Tomas Rusnak (trusnak)
	label-Code-Review = -2..+2 group osp-ceph-automation
	label-Code-Review = -2..+2 group user/Yogev Rabl (yorabl)
	label-Verified = -1..+1 group osp-ceph-automation
	label-Verified = -1..+1 group user/Yogev Rabl (yorabl)
	rebase = group osp-ceph-automation
	rebase = group user/Yogev Rabl (yorabl)
        rebase = group user/Tomas Rusnak (trusnak)
	removeReviewer = group osp-ceph-automation
	removeReviewer = group user/Yogev Rabl (yorabl)
        removeReviewer = group user/Tomas Rusnak (trusnak)
	submit = group osp-ceph-automation
	submit = group user/Yogev Rabl (yorabl)
        submit = group user/Tomas Rusnak (trusnak)
[access "refs/heads/*"]
	read = group osp-ceph-automation
	read = group user/Yogev Rabl (yorabl)
        read = group user/Tomas Rusnak (trusnak)
	create = group osp-ceph-automation
	create = group user/Yogev Rabl (yorabl)
        create = group user/Tomas Rusnak (trusnak)
	push = group osp-ceph-automation
	push = +force group user/Yogev Rabl (yorabl)
        push = +force group user/Tomas Rusnak (trusnak)
	pushMerge = group osp-ceph-automation
	pushMerge = group user/Yogev Rabl (yorabl)
        pushMerge = group user/Tomas Rusnak (trusnak)
[access "refs/tags/*"]
	push = group osp-ceph-automation
	push = group user/Yogev Rabl (yorabl)
	push = +force group user/Yogev Rabl (yorabl)
        push = +force group user/Tomas Rusnak (trusnak)
